;\r\nconst player = $get('Sprite::Player');\r\nconst map = $get('Tilemap::Level 1 Map');\r\n\r\nmainScene.onUpdate((delta) => {\r\n    screen.clear()\r\n\r\n    playerManager.updatePlayer(delta);\r\n});\r\n\r\nmainScene.onDraw(() => {\r\n    map.draw();\r\n    player.draw();\r\n    \r\n    // Uncomment this line to see the collision shapes\r\n    // mapCollisions.drawCollisionShapes();\r\n});","player.js":"import {$get} from './_out.js';\r\nimport {gameify} from '/gameify/gameify.js';\r\n\r\nimport * as mapCollisions from './mapCollisions.js';\r\n\r\n// player.js\r\n\r\nconst screen = $get('Screen::')[0];\r\nconst player = $get('Sprite::Player');\r\nconst map = $get('Tilemap::Level 1 Map');\r\n\r\nplayer.setShape(new gameify.shapes.Rectangle(6, 4, 60, 68));\r\nplayer.animator.set(\"walk\", $get('Animation::Player Walk'));\r\nplayer.animator.set(\"idle\", $get('Animation::Player Idle'));\r\nplayer.animator.set(\"jump\", $get('Animation::Player Jump'));\r\nplayer.animator.set(\"die\",  $get('Animation::Player Die'));\r\n\r\nconst playerSpeed = 160;      // pixels per second\r\nconst playerJumpForce = 400;  // px/s\r\nconst gravityAccel = 960;     // px/s^2\r\nconst terminalVelocity = 960; // px/s\r\nconst worldBottom = 620;      // die if you fall below here\r\n\r\nconst playerHeight = 72;\r\nlet playerOnGround = false;\r\nlet playerOnLadder = false;\r\n\r\nexport function updatePlayer(delta) {\r\n\r\n    // Check for keyboard buttons and set the player's\r\n    // velocity to move it\r\n    \r\n    if (screen.keyboard.keyIsPressed(\"D\")) {\r\n        player.velocity.x = playerSpeed;\r\n    } else if (screen.keyboard.keyIsPressed(\"A\")) {\r\n        player.velocity.x = -playerSpeed;\r\n    } else {\r\n        player.velocity.x = 0;\r\n    }\r\n    \r\n    if (screen.keyboard.keyIsPressed(\"W\") && playerOnGround) {\r\n        player.velocity.y = -playerJumpForce;\r\n    } else if (screen.keyboard.keyIsPressed(\"W\") && playerOnLadder) {\r\n        player.velocity.y = -playerSpeed;\r\n    } else if (screen.keyboard.keyIsPressed(\"S\") && playerOnLadder) {\r\n        player.velocity.y = playerSpeed;\r\n    }\r\n    \r\n    // convert milliseconds to seconds, because player.velocity is in px/s\r\n    player.velocity.y += gravityAccel*(delta/1000);\r\n    // If velocity is too high, the player will go through the floor\r\n    if (player.velocity.y > terminalVelocity) {\r\n        player.velocity.y = terminalVelocity;\r\n    }\r\n    \r\n    // Reset playerOnGround and playerOnLadder before\r\n    // checking for collisions\r\n    playerOnGround = false;\r\n    playerOnLadder = false;\r\n    \r\n    // Check for collisions, update player,\r\n    // and set playerOnGround and playerOnLadder\r\n    checkForCollisionsAndUpdate(delta);\r\n    \r\n    if(player.animator.currentAnimationName === \"die\") {\r\n        // Don't start another animation\r\n    } else if (player.velocity.x === 0 && playerOnGround) {\r\n        // on ground, not moving\r\n        player.animator.play(\"idle\");\r\n    } else if (playerOnGround) {\r\n        // on ground, moving\r\n        player.animator.play(\"walk\");\r\n    } else {\r\n        // not on ground\r\n        player.animator.play(\"jump\");\r\n    }\r\n    \r\n    if (player.position.y > worldBottom) {\r\n        lose();\r\n    }\r\n    \r\n    screen.camera.focus(player.position, {x:0, y:0});\r\n}\r\n\r\nexport function lose() {\r\n    player.animator.play(\"die\");\r\n    // back to beginning\r\n    player.position.x = 170;\r\n    player.position.y = 410;\r\n    // reset the camera to exactly the player\r\n    screen.camera.translateAbsolute(player.position);\r\n}\r\nexport function win() {\r\n    alert('You win!');\r\n    // back to beginning\r\n    player.position.x = 170;\r\n    player.position.y = 410;\r\n}\r\n\r\nfunction checkForCollisionsAndUpdate(delta) {\r\n    const lastPosition = player.position.copy();\r\n    const lastVelocity = player.velocity.copy();\r\n    let doneColliding = false;\r\n    \r\n    while (!doneColliding) {\r\n        doneColliding = true;\r\n        \r\n        // The update function takes the velocity and moves the player\r\n        player.update(delta, /*updateAnimator=*/false);\r\n        \r\n        // Find all (if any) tiles that the player collides with;\r\n        const colTiles = mapCollisions.collidesWithSprite(player);\r\n        for (const tile of colTiles) {\r\n            if (tile.shape) {\r\n                if (tile.tags.includes('ladder')) {\r\n                    playerOnLadder = true;  \r\n                    // skip other checks for this collision\r\n                    continue;\r\n                } else if (tile.tags.includes('spike')) {\r\n                    lose();\r\n                    return;\r\n                } else if (tile.tags.includes('victory')) {\r\n                    win();\r\n                    return;\r\n                }\r\n                \r\n                // revert to before collision, if the player does collide\r\n                player.position = lastPosition;\r\n                \r\n                if (tile.shape.position.y >= lastPosition.y + playerHeight) {\r\n                    // Try again with less gravity\r\n                    player.velocity.y /= 2;\r\n                    if (Math.abs(player.velocity.y) < 1) {\r\n                        // clear gravity if there's very little\r\n                        player.velocity.y = 0;\r\n                        playerOnGround = true;\r\n                    }\r\n                    doneColliding = false; // try again\r\n                } else if (player.velocity.x !== 0) {\r\n                    player.velocity.x = 0; // clear movement\r\n                    doneColliding = false; // try again w/o movement\r\n                }\r\n            }\r\n        }\r\n    }\r\n    \r\n    if (!playerOnGround) {\r\n        // Add back the velocity from last round (gravity, etc)\r\n        player.velocity.y = lastVelocity.y;\r\n    }\r\n    \r\n    // We do this to move the collision shape back\r\n    // to lastPosition (otherwise it might move when there's a collision)\r\n    // Comment this line, enable collision shape drawing,\r\n    // then jump on the side of a wall to see\r\n    player.update(0);\r\n    // Update animations, since we avoided that when calling player.update\r\n    player.animator.update(delta);\r\n}\r\n\r\n\r\n\r\n\r\n","style.css":"html, body {\r\n    background: #ddd;\r\n    height: 100%;\r\n    margin: 0;\r\n}\r\ndiv {\r\n    display: flex;\r\n    flex-direction: column;\r\n    justify-content: center;\r\n    align-items: center;\r\n    height: 100%;\r\n    width: 100%;\r\n    margin: 0;\r\n}\r\ncanvas {\r\n    position: relative;\r\n    background: white;\r\n    max-width: 100%;\r\n    max-height: 100%;\r\n}","index.html":"\r\n<!DOCTYPE html>\r\n<html lang=\"en\">\r\n    <head>\r\n        <meta charset=\"utf-8\">\r\n        <title>Gameify Game</title>\r\n        <link rel=\"stylesheet\" href=\"./style.css\">\r\n    </head>\r\n    <body>\r\n        <div>\r\n            <canvas id=\"game-canvas\"></canvas>\r\n        </div>\r\n    <script src=\"./main.js\" type=\"module\"></script>\r\n    </body>\r\n<html>","mapCollisions.js":"\nimport {$get} from './_out.js';\nimport {gameify} from '/gameify/gameify.js';\n\n// map_collisions.js\n\nconst player = $get('Sprite::Player');\nconst screen = $get('Screen::')[0];\nconst map = $get('Tilemap::Level 1 Map');\n\nscreen.camera.setSpeed(0.002);\nscreen.camera.minDistance = 100;\nscreen.camera.maxDistance = 400;\n\n// Check if the sprite collides with the map.\n// return false if not, or the shape it collided with if true.\nexport function collidesWithSprite(sprite) {\n    \n    const colTiles = [];\n    \n    // Only check tiles within 3 tiles of the player\n    const nearestTile = map.worldToMap(sprite.shape.position);\n    const maxDistance = 3\n    for (let x = nearestTile.x - maxDistance; x < nearestTile.x + maxDistance; x++) {\n        for (let y = nearestTile.y - maxDistance; y < nearestTile.y + maxDistance; y++) {\n            let tile = map.get(x, y);\n            \n            if (tile && tile.shape && tile.shape.collidesWith(sprite.shape)) {\n                tile.shape.fillColor = '#f008'; // Color shapes for debugging\n                colTiles.push(tile);\n            } else if (tile && tile.shape && tile.shape.fillColor == '#00f3') {\n                tile.shape.fillColor = '#00f8';\n            }\n        }\n    }\n    \n    return colTiles;\n}\n\nexport function drawCollisionShapes() {\n    \n    for (const tile of map.listTiles()) {\n        if (tile && tile.shape) {\n            tile.shape.draw(screen.context);\n            // reset the color after drawing, for debugging\n            tile.shape.fillColor = '#00f3';\n        }\n    }\n\n    player.shape.draw(screen.context);\n}\n\n"},"timestamp":1713752683627}